# Battery
genfscon sysfs /devices/platform/soc/c440000.qcom,spmi/spmi-0/spmi0-02/c440000.qcom,spmi:qcom,pm8150b@2:qpnp,fg/power_supply/bms/capacity_raw     u:object_r:sysfs_battery_supply:s0
genfscon sysfs /devices/platform/soc/880000.i2c/i2c-1/1-0061/power_supply/wireless/online                                                         u:object_r:sysfs_battery_supply:s0
genfscon sysfs /devices/platform/soc/880000.i2c/i2c-1/1-0061/power_supply/wireless/type                                                           u:object_r:sysfs_battery_supply:s0

# Light
genfscon sysfs /class/backlight/panel0-backlight/brightness                     u:object_r:sysfs_graphics:s0
genfscon sysfs /class/backlight/panel0-backlight-ex/brightness                  u:object_r:sysfs_graphics:s0
genfscon sysfs /class/backlight/panel0-backlight/max_brightness                 u:object_r:sysfs_graphics:s0
genfscon sysfs /class/backlight/panel0-backlight-ex/max_brightness              u:object_r:sysfs_graphics:s0
genfscon sysfs /class/lg_rgb_led/use_patterns/onoff_patterns                    u:object_r:sysfs_leds:s0
genfscon sysfs /class/lg_rgb_led/use_patterns/blink_patterns                    u:object_r:sysfs_leds:s0
genfscon sysfs /devices/virtual/lg_rgb_led/use_patterns/onoff_patterns          u:object_r:sysfs_leds:s0
genfscon sysfs /devices/virtual/lg_rgb_led/use_patterns/blink_patterns          u:object_r:sysfs_leds:s0

# Sensors
genfscon sysfs /devices/platform/soc/5c00000.qcom,ssc/subsys2/state    u:object_r:vendor_sysfs_sensors:s0

# Tap2wake
